{
	"useTabs": true,
	"singleQuote": true,
	"trailingComma": "none",
	"plugins": ["prettier-plugin-svelte"],
	"tabWidth": 2,
	"semi": false,
	"overrides": [
		{
			"files": "*.svelte",
			"options": {
				"parser": "svelte"
			}
		}
	],
	"printWidth": 100,
	"bracketSameLine": true,
	"htmlWhitespaceSensitivity": "ignore"
}
